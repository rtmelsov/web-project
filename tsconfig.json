{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "strict": true,
      "jsx": "react-jsx",
      "allowJs": true,
      "noUnusedLocals": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "moduleResolution": "node"
    },
    "ts-node": {
        "compilerOptions": {
          "module": "CommonJS"
        }
      }
  }